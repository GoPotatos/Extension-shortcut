{
"manifest_version":2,
"name":"Google Search",
"version":"1.0",
"browser_action":{
"default_title":"Search",
"default_icon":{

},
"icons":{

}
},
"content_scripts":[{
"js":["content.js"],
"matches":["<all_urls>"]
}],
"background":{
"scripts":["background.js"],
"matches":["<all_urls>"]
},
"commands": {
        "_execute_browser_action": {
            "suggested_key": {
                "default": "Alt+S"
            }
        }
    }
}